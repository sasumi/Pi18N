{
  "name": "lfphp/pi18n",
  "description": "A lite function pack for PHP.",
  "keywords": [
	"function"
  ],
  "homepage": "http://blog.yikepi.com/",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
	{
	  "name": "Sasumi.H",
	  "email": "cnsasumi@outlook.com",
	  "homepage": "http://blog.yikepi.com/"
	}
  ],
  "support": {
	"issues": "https://github.com/sasumi/Pi18N/issues"
  },
  "require": {
	"php": ">=5.6",
	"ext-mbstring": "*",
	"ext-gettext": "*",
	"lfphp/func": "~1.0.32",
	"ext-intl": "*"
  },
  "require-dev": {
	"phpunit/phpunit": "8.5.8"
  },
  "autoload": {
	"psr-4": {
	  "LFPhp\\Pi18N\\": "src"
	},
	"autoload-dev": {
	  "psr-4": {
		"LFPhp\\Pi18N\\": "src",
		"LFPhp\\Pi18N\\TestCase\\": "tests"
	  }
	}
  },
  "repositories": {
	"packagist": {
	  "type": "composer",
	  "url": "https://mirrors.aliyun.com/composer/"
	}
  }
}
